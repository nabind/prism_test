          OR (GV_CONTENT_CODE = '03' AND V_FIELD_NAME = 'OBJECTIVE_4_PERCENT_CORRECT-OBJ' AND V_FIELD_VAL IS NOT NULL)
          OR (GV_CONTENT_CODE = '03' AND V_FIELD_NAME = 'OBJECTIVE_5_PERCENT_CORRECT-OBJ' AND V_FIELD_VAL IS NOT NULL)
          OR (GV_CONTENT_CODE = '03' AND V_FIELD_NAME = 'OBJECTIVE_6_PERCENT_CORRECT-OBJ' AND V_FIELD_VAL IS NOT NULL)
          OR (GV_CONTENT_CODE = '03' AND V_FIELD_NAME = 'OBJECTIVE_7_PERCENT_CORRECT-OBJ' AND V_FIELD_VAL IS NOT NULL)
             ) THEN
            V_FLAG             := 'Y';
            OUT_FIELD_IS_VALID := V_FLAG;
            OUT_FIELD_NAME     := REGEXP_SUBSTR(V_FIELD_NAME, '[^-]+', 1, 1);
            OUT_FIELD_ERR_CODE := V_ERR_CODE;
          END IF;
          
          
      END IF;
  
  END IF; 
  
  END PRC_MO_VALID_VALUES_CHECK;

  PROCEDURE PRC_ACCOMMODATIONS_CODE(IN_VALUE IN VARCHAR2,
                                    OUT_VAL  OUT VARCHAR2) IS
  
    V_LEN NUMBER(20);
    V_VAL VARCHAR2(100);
  
  BEGIN
    V_VAL   := UPPER(IN_VALUE);
    V_LEN   := REGEXP_INSTR(V_VAL, '[^Y| ]+', 1, 1);
    OUT_VAL := 'Accom_' || V_LEN;
  
  END PRC_ACCOMMODATIONS_CODE;

END PKG_MO_STUDENT_DATA_VALIDATION;
