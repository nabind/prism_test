          WHERE STS.TASK_ID = (SELECT DISTINCT TASK_ID 
                                 FROM STUDENT_DATA_EXTRACT SD 
                                WHERE SD.FILENAME = FILE_NAME.FILENAME);
           COMMIT;
       
         END IF;
