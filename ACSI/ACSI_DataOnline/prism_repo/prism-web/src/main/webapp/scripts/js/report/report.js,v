function resetTimer() {
	// reset timer
	clearTimeout(changeInt);
	// Init
	seconds = 0;
	// Start counter
	changeInt = setTimeout(reserAlert, 1000);
}
function reserAlert() {
	++seconds;
	if(seconds > alertInterval) { 
		// show confirm
		keepAliveACSI();
	} else {
		// Next count
		changeInt = setTimeout(reserAlert, 1000);
	}
}
